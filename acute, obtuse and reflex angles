angle = int(input('Type an angle between 0 and 360 degrees: '))
if angle >= 0 and angle < 90:
  print('This an ancute angle')
elif angle == 90:
  print('This an right angle')
elif angle > 90 and angle < 180:
  print('This an abtuse angle')
elif angle == 180:
  print('This an staright angle')
elif angle > 180 and angle <= 360:
  print('This an reflex angle')
else:
  print('Try again with a value between 0 and 360 degrees')
